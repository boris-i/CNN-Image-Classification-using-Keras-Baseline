     ]
    }
   ],
   "source": [
    "(x_train, y_train), (x_test, y_test) = tf.keras.datasets.cifar10.load_data()\n",
    "y_test = tf.keras.utils.to_categorical(y_test)\n",
    "y_train = tf.keras.utils.to_categorical(y_train)\n",
    "\n",
    "class_names = ['airplane', 'car', 'bird', 'cat' , 'deer', 'dog', 'frog', 'horse', 'ship', 'truck']\n",
    "\n",
    "print(x_train.shape, y_train.shape)\n",
    "print(x_test.shape, y_test.shape)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Image Examples"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def show_examples(x, y, p = None):\n",
    "    indices = np.random.choice(range(x.shape[0]), 10, replace = False)\n",
    "    x = x[indices]\n",
    "    y = y[indices]\n",
    "    if not isinstance(p, np.ndarray):\n",
    "        p = y\n",
    "    else:\n",
    "        p = p[indices]\n",
    "    \n",
    "    plt.figure(figsize=(4,4))\n",
    "    for i in range(10):\n",
    "        plt.subplot(2,5,1+i)\n",
    "        plt.imshow(x[i])\n",
    "        plt.xticks([])\n",
    "        plt.yticks([])\n",
    "        col = 'green' if np.argmax(y[i]) == np.argmax(p[i]) else 'red'\n",
    "        \n",
    "        name = class_names[np.argmax(p[i])]\n",
    "        \n",
    "        if col == 'red':\n",
    "            name = name + \"\\n(\" + class_names[np.argmax(y[i])] + \")\"\n",
    "        plt.xlabel(name, color = col )\n",
    "        \n",
    "    plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 288x288 with 10 Axes>"
      ]
     },
